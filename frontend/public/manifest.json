{
  "manifest_version": 3,
  "name": "PassAssigner",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open Popup"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs", "identity"],
  "host_permissions": ["http://127.0.0.1:8000/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
